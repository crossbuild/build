#export PATH=$srcdir/bin
export PKG_CONFIG_PATH=$srcdir/$target/sys-root/usr/$lib/pkgconfig:$srcdir/$target/sys-root/usr/share/pkgconfig
export PKG_CONFIG_SYSTEM_LIBRARY_PATH=/usr/$lib
export PKG_CONFIG_SYSROOT_DIR=$srcdir/$target/sys-root
#( cd $srcdir/src && ./autogen.sh )
#echo 'EXTRA_DIST =' > $srcdir/src/docs/gtk-doc.make
. $recipes/cmi $srcdir/src \
    cc_cv_LDFLAGS__Wl__fuse_ld_gold=no \
    KILL=/usr/bin/kill \
    KMOD=/usr/bin/kmod \
    MOUNT_PATH=/usr/bin/mount \
    UMOUNT_PATH=/usr/bin/umount \
    --enable-lz4 \
    --disable-nls \
    --disable-gcrypt \
    --disable-gudev \
    --disable-libidn \
    --disable-manpages \
    --without-python \
    --disable-gnuefi

$recipes/strip_debug --strip-all $target $destdir /usr/lib/debug /
